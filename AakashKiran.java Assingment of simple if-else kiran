package kiransai;

import java.util.Scanner;

public class AakashKiran {
public static void main(String[] args) {
  Scanner scan=new Scanner(System.in);
  int i=scan.nextInt();
	if(i>7)
	{
		System.out.println("condition is right");
		{
			if(i>=7)
			{
				System.out.println("i am kiran");
			}
			else
			{
			   System.out.println("come to college");
			}
		}
	}
		else
		{
			System.out.println("i am bad boy");
		}
	}
}

